{
  "name": "db-migrate",
  "version": "1.0.0",
  "description": "A package to handle database migrations",
  "main": "index.js",
  "bin": {
    "db-migrate-x": "./src/bin/cli.js"
  },
  "scripts": {
    "setup": "node src/bin/cli.js setup",
