    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs.promises": "^0.1.2",
    "mysql2": "^3.11.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
